{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "start-vscode-extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/distribution/definy-extension",
        "--extensionDevelopmentKind=web"
      ],
      "preLaunchTask": "vscode-extension-build"
    }
  ]
}
